<h1>Sessions</h1>
<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= form_with model: @user, url: session do |form| %>
    <%= form.label :email %><br/>
    <%= form.text_field :email %><br/>
    <%= form.label :password %><br/>
    <%= form.password_field :password %><br/>
    <%= form.submit %>

<% end %>
